@using System.Threading.Tasks
@using AdminNetBaires.Entities
@using AdminNetBaires.ViewModels
@model MembersIndexViewModel
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Home</title>
</head>
<body>
<h1>Lista de Members</h1>

<table>
    @foreach (Member item in Model.Members)
    {
        <tr>
            <td>@Html.ActionLink(item.Id.ToString(), "Details", new { id = item.Id })</td>
            <td>@item.Name</td>
            <td>@item.LastName</td>
            <td>@item.Email</td>
        </tr>
    }
</table>
</body>
</html>